<div class="well">

	
	<%=form_for ([@user ,@reporte]) do |f|%>

		<table class="table">
			
			<tbody>
				<div class="form-group">
					<tr>
						<th width="5px">
							<%=f.label :cedula%> 
							
						</th>
						<th>
							<%=f.text_field :cedula%>
						</th>

					</tr>
				</div>
					<div class="form-group">
						<tr>
							<th>
								<%=f.label :nombre, "Nombres"%>
							</th>
							<th>
								<%=f.text_field :nombre%>
							</th>

						</tr>
					</div>
					<div class="form-group">
						<tr>
							<th>
								<%=f.label :apellido1%>
							</th>
							<th>
								<%=f.text_field :apellido1%>
							</th>

						</tr>
					</div>
					<div class="form-group">
						<tr>
							<th>
								<%=f.label :apellido2%>
							</th>
							<th>
								<%=f.text_field :apellido2%>
							</th>

						</tr>
					</div>
					<div class="form-group">
						<tr>
							<th>
								<%=f.label :dependencia, "Cargo Laboral"%>
							</th>
							<th>
								<%=f.text_field :dependencia%>
							</th>

						</tr>
					</div>

					<div class="form-group">
						<tr>
							<th>
								<%=f.label :telefono%>
							</th>
							<th>
								<%=f.text_field :telefono%>
							</th>

						</tr>
					</div>
					<div class="form-group">
						<tr>
							<th>
								<%=f.label :asociado%>
							</th>
							<th>
								<%=f.text_field :asociado%>
							</th>

						</tr>
					</div>

					<div class="form-group">
						<tr>
							<th>
								<%=f.label :resultado%>
							</th>
							<th>
								<%=f.text_area :resultado%>
							</th>

						</tr>
					</div>
					<div class="form-group">
						<tr>
							<th>
								<%=f.label :referido, "Cuantos Referidos"%>	
							</th>
							<th>
								<%=f.text_field :referido%>
							</th>

						</tr>
					</div>
						
				<tr>
					<td>
						<%=f.submit%>
					</td>
				</tr>	
			</tbody>		
		</table>

	<%end%>

</div>